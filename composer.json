{
    "name": "juanrube/textdiff",
    "description": "Laravel package for text diff highlighting using jfcherng/php-diff",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Juanrube\\TextDiff\\": "src/"
        }
    },
    "require": {
        "php": "^8.1|^8.2|^8.3",
        "jfcherng/php-diff": "^7.0",
        "illuminate/support": "^9.0|^10.0|^11.0|^12.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Juanrube\\TextDiff\\TextDiffServiceProvider"
            ],
            "aliases": {
                "TextDiff": "Juanrube\\TextDiff\\Facades\\TextDiff"
            }
        },
        "branch-alias": {
            "dev-support-laravel-8-php74": "1.x-dev"
        }
    },
    "minimum-stability": "alpha",
    "prefer-stable": true
}
